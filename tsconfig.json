{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2015"],
    "types": [],
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "checkJs": false,
    "skipLibCheck": true,
    "exactOptionalPropertyTypes": false,
    "declaration": true,
    "declarationMap": true
  },
  "include": [
    "entry-server.ts",
    ".eslintrc.js",
  ],
  "exclude": ["src/client"]
}
